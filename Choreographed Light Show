// Start at 0:22
int black = 8;
int red = 7;
int purple = 6;
int white = 5;
int blue = 4;
int green = 3;
int orange = 2;
void setup(){
  pinMode(8, OUTPUT);
  pinMode(7, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(2, OUTPUT);
}

void loop() {  
  digitalWrite(green, HIGH);
  delay(200); 
  digitalWrite(green, LOW);
  delay(75);
  digitalWrite(blue, HIGH);
  delay(100);
  digitalWrite(blue, LOW);
  delay(100);
  digitalWrite(white, HIGH);
  delay(100);
  digitalWrite(white, LOW);
  delay(100);
  digitalWrite(black, HIGH);
  delay(75);
  digitalWrite(red, HIGH);
  delay(75);
  digitalWrite(black, LOW);
  delay(75);
  digitalWrite(red, LOW);
  delay(75); 
  //2nd time
  digitalWrite(green, HIGH);
  delay(200); 
  digitalWrite(green, LOW);
  delay(75);
  digitalWrite(blue, HIGH);
  delay(100);
  digitalWrite(blue, LOW);
  delay(100);
  digitalWrite(white, HIGH);
  delay(100);
  digitalWrite(white, LOW);
